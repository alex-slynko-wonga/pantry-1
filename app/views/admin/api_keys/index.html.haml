%h1 Api Keys

%table.table
  %thead
    %tr
      %th #
      %th Name
      %th Api Key
  %tbody
    - @api_keys.each_with_index do |api_key, index|
      %tr
        %td= index + 1
        %td= link_to api_key.name, edit_admin_api_key_url(api_key)
        %td= api_key.key

= link_to 'New Api Key', new_admin_api_key_url
