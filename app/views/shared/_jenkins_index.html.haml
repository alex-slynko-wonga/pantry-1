%table.table
  %thead
    %tr
      %th #
      %th InstanceID
      %th Name
      %th ImageID
      %th Groups
      %th bootstrapped
      %th Booted
      %th Joined
    - jenkins_instances.each do |jenkins_instance|
      %tr
        %td= link_to jenkins_instance.id, [jenkins_server, jenkins_instance]
        %td= jenkins_instance.ec2_instance.id
        %td= jenkins_instance.ec2_instance.name
        %td= jenkins_instance.ec2_instance.ami
        %td= jenkins_instance.ec2_instance.security_group_ids
        %td= jenkins_instance.ec2_instance.bootstrapped
        %td= jenkins_instance.ec2_instance.booted
        %td= jenkins_instance.ec2_instance.joined

        