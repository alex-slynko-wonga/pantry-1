%table.table
  %thead
    %tr
      %th #
      %th Name
      %th Version
      %th Url
      %th Date added
  - @packages.each_with_index do |package, index|
    %tr
      %td= index + 1
      %td= package.name
      %td= package.version
      %td= link_to package.url, package.url
      %td= l package.created_at, format: :long
