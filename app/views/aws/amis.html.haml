%h1 AMI
%table.table
  %thead
    %tr
      %th #
      %th TagSet
      %th ImageID
      %th ImageState
      %th Architecture
      %th ImageType
      %th Platform
      %th Name
      %th Description
    - @lists.each_with_index do |list,index|
      %tr
        %td= index + 1
        %td= list["tagSet"]
        %td= list["imageId"]
        %td= list["imageState"]
        %td= list["architecture"]
        %td= list["imageType"]
        %td= list["platform"]
        %td= list["name"]
        %td= list["description"]