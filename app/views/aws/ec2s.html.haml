%h1 EC2 Servers
%table.table
  %thead
    %tr
      %th #
      %th ID
      %th ImageID
      %th Groups
      %th State
      %th Tags
    - @lists.each_with_index do |list,index|
      %tr
        %td= index + 1
        %td= list.id
        %td= list.image_id
        %td= list.groups
        %td= list.state
        %td= list.tags

        